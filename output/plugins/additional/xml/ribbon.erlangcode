		<template name="evt_connect_OnRibbonGalleryExtensionButtonPressed"> %% event #class:wxEVT_COMMAND_BUTTON_CLICKED isn't currently supportedby Erlang</template>
	</templates>

	<templates class="ribbonGalleryItem">
		<template name="construction">
			#parent #class:append(#parent $name, $bitmap, $id);
		</template>
